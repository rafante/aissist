# AIssist Production Environment Variables
# These will be configured in Coolify after database service deployment

# Server identification
SERVERPOD_SERVER_ID=aissist-production

# API Server configuration
SERVERPOD_API_SERVER_PORT=8080
SERVERPOD_API_SERVER_PUBLIC_HOST=aissist.rafante-tec.online
SERVERPOD_API_SERVER_PUBLIC_PORT=443
SERVERPOD_API_SERVER_PUBLIC_SCHEME=https

# Insights Server configuration  
SERVERPOD_INSIGHTS_SERVER_PORT=8081
SERVERPOD_INSIGHTS_SERVER_PUBLIC_HOST=insights.aissist.rafante-tec.online
SERVERPOD_INSIGHTS_SERVER_PUBLIC_PORT=443
SERVERPOD_INSIGHTS_SERVER_PUBLIC_SCHEME=https

# Web Server configuration
SERVERPOD_WEB_SERVER_PORT=8082
SERVERPOD_WEB_SERVER_PUBLIC_HOST=ws.aissist.rafante-tec.online
SERVERPOD_WEB_SERVER_PUBLIC_PORT=443
SERVERPOD_WEB_SERVER_PUBLIC_SCHEME=wss

# External Database configuration (from separate Coolify service)
# UPDATE THESE HOST IDs AFTER DATABASE SERVICE DEPLOYMENT
SERVERPOD_DATABASE_HOST=<POSTGRES_CONTAINER_ID_FROM_COOLIFY>
SERVERPOD_DATABASE_PORT=5432
SERVERPOD_DATABASE_NAME=aissist
SERVERPOD_DATABASE_USER=postgres
SERVERPOD_DATABASE_REQUIRE_SSL=false

# External Redis configuration (from separate Coolify service)  
# UPDATE THESE HOST IDs AFTER DATABASE SERVICE DEPLOYMENT
SERVERPOD_REDIS_ENABLED=true
SERVERPOD_REDIS_HOST=<REDIS_CONTAINER_ID_FROM_COOLIFY>
SERVERPOD_REDIS_PORT=6379
SERVERPOD_REDIS_USER=

# Database and Redis passwords (set in Coolify environment)
SERVERPOD_PASSWORD_database=AiSsIsT2024!SecurePassword
SERVERPOD_PASSWORD_redis=AiSsIsT2024!RedisPassword

# Application settings
SERVERPOD_MAX_REQUEST_SIZE=524288
SERVERPOD_SESSION_PERSISTENT_LOG_ENABLED=true
SERVERPOD_SESSION_CONSOLE_LOG_ENABLED=false
SERVERPOD_SESSION_CONSOLE_LOG_FORMAT=json

# Future calls configuration
SERVERPOD_FUTURE_CALL_EXECUTION_ENABLED=true
SERVERPOD_FUTURE_CALL_CONCURRENCY_LIMIT=5
SERVERPOD_FUTURE_CALL_SCAN_INTERVAL=5000

# External API keys
TMDB_API_KEY=466fd9ba21e369cd51e7743d32b7833f

# Custom app secrets (generate new ones for production)
SERVERPOD_PASSWORD_JWT_SECRET=AiSsIsT2024!JwtSuperSecretKey
SERVERPOD_PASSWORD_API_SECRET=AiSsIsT2024!ApiSecretKey